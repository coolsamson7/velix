{
  "type": "container",
  "children": [
    {
      "type": "column",
      "mainAxisAlignment": "start",
      "crossAxisAlignment": "start",
      "mainAxisSize": "min",
      "children": [
        {
          "type": "grid",

          "rows": [
            {
              "sizeMode": "auto",
              "size": 1.0,
              "alignment": "start"
            },
            {
              "sizeMode": "auto",
              "size": 1.0,
              "alignment": "start"
            }
          ],
          "cols": [
            {
              "sizeMode": "auto",
              "size": 1.0,
              "alignment": "start"
            },
            {
              "sizeMode": "auto",
              "size": 1.0,
              "alignment": "start"
            }
          ],
          "spacing": 10,
          "children": [
            {
              "type": "text",
              "cell": {
                "row": 0,
                "col": 0
              },
              "children": [],
              "label": "0/0"
            },
            {
              "type": "text",
              "cell": {
                "row": 0,
                "col": 1
              },
              "children": [],
              "label": "0/1"
            },
            {
              "type": "text",
              "cell": {
                "row": 1,
                "col": 0
              },
              "children": [],
              "label": "1/0"
            },
            {
              "type": "text",
              "cell": {
                "row": 1,
                "col": 1
              },
              "children": [],
              "label": "1/1"
            }
          ]
        },

        {
          "type": "text",
          "databinding": "user.name",
          "children": [],
          "label": "Name"
        },
        {
          "type": "text",
          "databinding": "user.address.city",
          "children": [],
          "label": "City"
        },
        {
          "type": "text",
          "databinding": "user.address.street",
          "children": [],
          "label": "Street"
        },
        {
          "type": "switch",
          "databinding": "user.single",
          "children": [],
          "label": "Single"
        },
        {
          "type": "label",
          "label": {
            "type": "binding",
            "value": "user.name"
          },
          "children": []
        },
        {
          "type": "button",
          "children": [],
          "label": {
            "type":  "value",
            "value": "Save"
          },
          "onClick": "this.save()",
          "color": "FFFF0000",
          "padding": {
            "left": 1,
            "top": 1,
            "right": 1,
            "bottom": 1
          }
        }
      ]
    }
  ]
}